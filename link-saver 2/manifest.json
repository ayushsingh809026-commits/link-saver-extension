{
  "manifest_version": 3,
  "name": "Link Saver â€” MVP",
  "version": "0.1.0",
  "description": "Save current tab and view your saved links.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Open Link Saver"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "storage",
    "contextMenus"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}